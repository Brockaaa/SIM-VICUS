				</OutputDefinition>
				<OutputDefinition>
					<Quantity>OutletNodeTemperature</Quantity>
					<TimeType>None</TimeType>
					<ObjectListName>the objects</ObjectListName>
					<GridName>hourly</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>PressureDifference</Quantity>
					<TimeType>None</TimeType>
					<ObjectListName>the objects</ObjectListName>
					<GridName>hourly</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>ControllerResultValue</Quantity>
					<TimeType>None</TimeType>
					<ObjectListName>the objects</ObjectListName>
					<GridName>hourly</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>TemperatureDifference</Quantity>
					<TimeType>None</TimeType>
					<ObjectListName>the objects</ObjectListName>
					<GridName>hourly</GridName>
				</OutputDefinition>				
			</Definitions>
			<Grids>
				<OutputGrid name="hourly">
					<Intervals>
						<Interval>
							<IBK:Parameter name="StepSize" unit="h">1</IBK:Parameter>
						</Interval>
					</Intervals>
				</OutputGrid>
			</Grids>
			<TimeUnit>h</TimeUnit>
		</Outputs>
		<ObjectLists>
			<ObjectList name="the objects">
				<FilterID>*</FilterID>
				<ReferenceType>NetworkElement</ReferenceType>
			</ObjectList>
		</ObjectLists>
	</Project>
</NandradProject>
