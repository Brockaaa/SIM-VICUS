<?xml version="1.0" encoding="UTF-8" ?>
<NandradProject fileVersion="1.99">
	<Project>
		<ProjectInfo>
			<Comment>Internal loads test: two zones, constant loads, 75% radiant</Comment>
		</ProjectInfo>

		<Location>
			<IBK:Parameter name="Latitude" unit="Deg">51</IBK:Parameter>
			<IBK:Parameter name="Longitude" unit="Deg">13</IBK:Parameter>
			<IBK:Parameter name="Albedo" unit="---">0.2</IBK:Parameter>
			<ClimateFileName>${Project Directory}/climate/Konstantopol_20C.c6b</ClimateFileName>
		</Location>

		<SimulationParameter>
			<IBK:Parameter name="InitialTemperature" unit="C">20</IBK:Parameter>
			<Interval>
				<IBK:Parameter name="End" unit="min">10</IBK:Parameter>
			</Interval>
		</SimulationParameter>

		<SolverParameter>
			<IBK:Parameter name="DiscStretchFactor" unit="---">4</IBK:Parameter>
			<IBK:Parameter name="DiscMinDx" unit="mm">2</IBK:Parameter>
			<IBK:Parameter name="InitialTimeStep" unit="s">0.01</IBK:Parameter>
		</SolverParameter>


		<Zones>
			<Zone id="1" displayName="Room" type="Active">
				<IBK:Parameter name="Area" unit="m2">10</IBK:Parameter>
				<IBK:Parameter name="Volume" unit="m3">90</IBK:Parameter>
			</Zone>
			<Zone id="2" displayName="Room" type="Active">
				<IBK:Parameter name="Area" unit="m2">20</IBK:Parameter>
				<IBK:Parameter name="Volume" unit="m3">90</IBK:Parameter>
			</Zone>
		</Zones>

		<ConstructionInstances>
			<ConstructionInstance id="1" displayName="South">
				<ConstructionTypeId>10001</ConstructionTypeId>
				<IBK:Parameter name="Orientation" unit="Deg">180</IBK:Parameter>
				<IBK:Parameter name="Inclination" unit="Deg">90</IBK:Parameter>
				<IBK:Parameter name="Area" unit="m2">10</IBK:Parameter>
				<InterfaceA id="1" zoneId="1">
						<!--Interface to 'Room'-->
						<InterfaceHeatConduction modelType="Constant">
							<IBK:Parameter name="HeatTransferCoefficient" unit="W/m2K">1</IBK:Parameter>
						</InterfaceHeatConduction>
				</InterfaceA>
			</ConstructionInstance>
			<ConstructionInstance id="2" displayName="South">
				<ConstructionTypeId>10001</ConstructionTypeId>
				<IBK:Parameter name="Orientation" unit="Deg">180</IBK:Parameter>
				<IBK:Parameter name="Inclination" unit="Deg">90</IBK:Parameter>
				<IBK:Parameter name="Area" unit="m2">10</IBK:Parameter>
				<InterfaceB id="1" zoneId="2">
						<!--Interface to 'Room'-->
						<InterfaceHeatConduction modelType="Constant">
							<IBK:Parameter name="HeatTransferCoefficient" unit="W/m2K">1</IBK:Parameter>
						</InterfaceHeatConduction>
				</InterfaceB>
			</ConstructionInstance>
		</ConstructionInstances>
		
		
		<ConstructionTypes>
			<ConstructionType id="10001" displayName="Inside insulated construction">
				<MaterialLayers>
					<MaterialLayer thickness="0.04" matId="1002" />
					<MaterialLayer thickness="0.12" matId="1001" />
				</MaterialLayers>
			</ConstructionType>
		</ConstructionTypes>
		
		
		<Materials>
			<Material id="1001" displayName="Brick">
				<IBK:Parameter name="Density" unit="kg/m3">2000</IBK:Parameter>
				<IBK:Parameter name="HeatCapacity" unit="J/kgK">1000</IBK:Parameter>
				<IBK:Parameter name="Conductivity" unit="W/mK">1.2</IBK:Parameter>
			</Material>
			<Material id="1002" displayName="Insulation">
				<IBK:Parameter name="Density" unit="kg/m3">100</IBK:Parameter>
				<IBK:Parameter name="HeatCapacity" unit="J/kgK">1000</IBK:Parameter>
				<IBK:Parameter name="Conductivity" unit="W/mK">0.04</IBK:Parameter>
			</Material>
		</Materials>
		
		
		<Models>
			<InternalLoadsModel id="401" displayName="Zone loads" modelType="Constant">
				<ZoneObjectList>All zones</ZoneObjectList>
				<IBK:Parameter name="EquipmentRadiationFraction" unit="---">0.75</IBK:Parameter>
				<IBK:Parameter name="PersonRadiationFraction" unit="---">0.75</IBK:Parameter>
				<IBK:Parameter name="LightingRadiationFraction" unit="---">0.75</IBK:Parameter>
				<IBK:Parameter name="EquipmentHeatLoadPerArea" unit="W/m2">150</IBK:Parameter>
				<IBK:Parameter name="PersonHeatLoadPerArea" unit="W/m2">80</IBK:Parameter>
				<IBK:Parameter name="LightingHeatLoadPerArea" unit="W/m2">60</IBK:Parameter>
			</InternalLoadsModel>
		</Models>
		

		<Outputs>
			<Definitions>
				<!-- zone-specific outputs -->
				<OutputDefinition>
					<Quantity>AirTemperature</Quantity>
					<ObjectListName>All zones</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>ConvectiveEquipmentHeatLoad</Quantity>
					<ObjectListName>All zones</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>ConvectivePersonHeatLoad</Quantity>
					<ObjectListName>All zones</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>ConvectiveLightingHeatLoad</Quantity>
					<ObjectListName>All zones</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				
				
				<!-- internal loads model outputs -->
				<OutputDefinition>
					<Quantity>ConvectiveEquipmentHeatLoad[id=1]</Quantity>
					<ObjectListName>Internal load models</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>ConvectivePersonHeatLoad[id=1]</Quantity>
					<ObjectListName>Internal load models</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>ConvectiveLightingHeatLoad[id=1]</Quantity>
					<ObjectListName>Internal load models</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>

				<OutputDefinition>
					<Quantity>RadiantEquipmentHeatLoad[id=1]</Quantity>
					<ObjectListName>Internal load models</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>RadiantPersonHeatLoad[id=1]</Quantity>
					<ObjectListName>Internal load models</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>RadiantLightingHeatLoad[id=1]</Quantity>
					<ObjectListName>Internal load models</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>

				<!-- zone 2 -->
				<OutputDefinition>
					<Quantity>ConvectiveEquipmentHeatLoad[id=2]</Quantity>
					<ObjectListName>Internal load models</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>ConvectivePersonHeatLoad[id=2]</Quantity>
					<ObjectListName>Internal load models</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>ConvectiveLightingHeatLoad[id=2]</Quantity>
					<ObjectListName>Internal load models</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>

				<OutputDefinition>
					<Quantity>RadiantEquipmentHeatLoad[id=2]</Quantity>
					<ObjectListName>Internal load models</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>RadiantPersonHeatLoad[id=2]</Quantity>
					<ObjectListName>Internal load models</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>RadiantLightingHeatLoad[id=2]</Quantity>
					<ObjectListName>Internal load models</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>

				<!-- construction instance outputs -->
				<OutputDefinition>
					<Quantity>SurfaceTemperatureA</Quantity>
					<ObjectListName>Walls</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>SurfaceTemperatureB</Quantity>
					<ObjectListName>Walls</ObjectListName>
					<GridName>5 mins</GridName>
				</OutputDefinition>
				
			</Definitions>
			
			<Grids>
				<OutputGrid name="5 mins">
					<Intervals>
						<Interval>
							<IBK:Parameter name="StepSize" unit="min">5</IBK:Parameter>
						</Interval>
					</Intervals>
				</OutputGrid>
			</Grids>
		</Outputs>
		<ObjectLists>
			<ObjectList name="All zones">
				<FilterID>*</FilterID>
				<ReferenceType>Zone</ReferenceType>
			</ObjectList>
			<ObjectList name="Internal load models">
				<FilterID>401</FilterID>
				<ReferenceType>Model</ReferenceType>
			</ObjectList>
			<ObjectList name="Walls">
				<FilterID>*</FilterID>
				<ReferenceType>ConstructionInstance</ReferenceType>
			</ObjectList>
		</ObjectLists>
	</Project>
</NandradProject>
