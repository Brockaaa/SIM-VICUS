<?xml version="1.0" encoding="UTF-8" ?>
<NandradProject fileVersion="1.99">
	<Project>
		<ProjectInfo>
			<Comment>SimQuality TestCase 02</Comment>
			<Created>2020-08-14</Created>
			<LastEdited>2020-08-14</LastEdited>
		</ProjectInfo>
		<Zones>
			<Zone id="1" displayName="Room" type="Active">
				<IBK:Parameter name="Temperature" unit="C">5</IBK:Parameter>
				<IBK:Parameter name="Area" unit="m2">10</IBK:Parameter>
				<IBK:Parameter name="Volume" unit="m3">30</IBK:Parameter>
			</Zone>
		</Zones>
		<SolverParameter>
			<Integrator>ExplicitEuler</Integrator>
			<IBK:Parameter name="InitialTimeStep" unit="s">60</IBK:Parameter>
		</SolverParameter>
		<Location>
			<IBK:Parameter name="Albedo" unit="---">0.2</IBK:Parameter>
			<IBK:Flag name="PerezDiffuseRadiationModel">true</IBK:Flag>
			<ClimateFileName>${Project Directory}/climate/DEU_Potsdam_60.epw</ClimateFileName>
			<Sensors>
				<!-- Radiation sensons automatically provide sensor quantities like DirectSWRadOnPlane, DiffuseSWRadOnPlane, GlobalSWRadOnPlane and IncidenceAngle, which
					can be queried as vector-valued results of the LOCATION object using the sensor ID as modelID-index. -->

				<!-- Flat roof -->
				<Sensor id="1000">
					<IBK:Parameter name="Orientation" unit="Deg">0</IBK:Parameter>
					<IBK:Parameter name="Inclination" unit="Deg">0</IBK:Parameter>
				</Sensor>
				<!-- North wall -->
				<Sensor id="100">
					<IBK:Parameter name="Orientation" unit="Deg">0</IBK:Parameter>
					<IBK:Parameter name="Inclination" unit="Deg">90</IBK:Parameter>
				</Sensor>
				<!-- North roof -->
				<Sensor id="101">
					<IBK:Parameter name="Orientation" unit="Deg">0</IBK:Parameter>
					<IBK:Parameter name="Inclination" unit="Deg">30</IBK:Parameter>
				</Sensor>
			</Sensors>
		</Location>
		<Outputs>
			<TimeUnit>min</TimeUnit>
			<OutputDefinitions>
				<!-- minutely values -->
				<OutputDefinition>
					<Quantity>DirectSWRadOnPlane[1000]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>minutely</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>DiffuseSWRadOnPlane[1000]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>minutely</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>DirectSWRadOnPlane[100]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>minutely</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>DiffuseSWRadOnPlane[100]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>minutely</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>DirectSWRadOnPlane[101]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>minutely</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>DiffuseSWRadOnPlane[101]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>minutely</GridName>
				</OutputDefinition>
				<!-- hourly values -->
				<OutputDefinition>
					<Quantity>DirectSWRadOnPlane[1000]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>hourly</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>DiffuseSWRadOnPlane[1000]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>hourly</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>DirectSWRadOnPlane[100]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>hourly</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>DiffuseSWRadOnPlane[100]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>hourly</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>DirectSWRadOnPlane[101]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>hourly</GridName>
				</OutputDefinition>
				<OutputDefinition>
					<Quantity>DiffuseSWRadOnPlane[101]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>hourly</GridName>
				</OutputDefinition>
				<!-- integral values -->
				<OutputDefinition>
					<Filename>load_integrals</Filename>
					<Quantity>DirectSWRadOnPlane[1000]</Quantity>
					<ObjectListName>Location</ObjectListName>
					<GridName>hourly</GridName>
					<TimeType>Integral</TimeType>
				</OutputDefinition>
			</OutputDefinitions>
			<Grids>
				<OutputGrid name="hourly">
					<Intervals>
						<Interval>
							<IBK:Parameter name="StepSize" unit="h">1</IBK:Parameter>
						</Interval>
					</Intervals>
				</OutputGrid>
				<OutputGrid name="mix">
					<Intervals>
						<Interval>
							<IBK:Parameter name="StepSize" unit="h">1</IBK:Parameter>
						</Interval>
					</Intervals>
				</OutputGrid>
				<OutputGrid name="minutely">
					<Intervals>
						<!-- 5. MÃ¤r -->
						<Interval>
							<IBK:Parameter name="Start" unit="min">90720</IBK:Parameter>
							<IBK:Parameter name="End" unit="min">92160</IBK:Parameter>
							<IBK:Parameter name="StepSize" unit="min">1</IBK:Parameter>
						</Interval>
						<!-- 27. Jul -->
						<Interval>
							<IBK:Parameter name="Start" unit="min">298080</IBK:Parameter>
							<IBK:Parameter name="End" unit="min">299520</IBK:Parameter>
							<IBK:Parameter name="StepSize" unit="min">1</IBK:Parameter>
						</Interval>
						<!-- 22. Sep -->
						<Interval>
							<IBK:Parameter name="Start" unit="min">380160</IBK:Parameter>
							<IBK:Parameter name="End" unit="min">381600</IBK:Parameter>
							<IBK:Parameter name="StepSize" unit="min">1</IBK:Parameter>
						</Interval>
						<!-- 24. Okt -->
						<Interval>
							<IBK:Parameter name="Start" unit="min">426240</IBK:Parameter>
							<IBK:Parameter name="End" unit="min">427680</IBK:Parameter>
							<IBK:Parameter name="StepSize" unit="min">1</IBK:Parameter>
						</Interval>
						<!-- 17. Dez -->
						<Interval>
							<IBK:Parameter name="Start" unit="min">504000</IBK:Parameter>
							<IBK:Parameter name="End" unit="min">505440</IBK:Parameter>
							<IBK:Parameter name="StepSize" unit="min">1</IBK:Parameter>
						</Interval>
					</Intervals>
				</OutputGrid>
			</Grids>
		</Outputs>
		<ObjectLists>
			<ObjectList name="All zones">
				<FilterID>*</FilterID>
				<ReferenceType>Zone</ReferenceType>
			</ObjectList>
			<ObjectList name="Location">
				<FilterID>*</FilterID>
				<ReferenceType>Location</ReferenceType>
			</ObjectList>
		</ObjectLists>
	</Project>
</NandradProject>
