						<DailyCycles>
							<DailyCycle interpolation="Constant">
								<TimePoints>0 6 20</TimePoints>
								<Values>InfiltrationRateSchedule [1/h]:2 0.5 2</Values>
							</DailyCycle>
						</DailyCycles>
					</Schedule>
